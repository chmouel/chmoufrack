  <div class="well-sm">
    <h3 class="text-primary text-center">{{.WP.Repetition}} * {{.WP.Meters}}m @ {{.WP.Percentage}}%</h3>
  </div>
  <div class="table-responsive">
  <table summary="Frac" class="table table-bordered table-striped">
    <thead>
      <tr>
        <th>VMA
        </th>
        <th>Total time</th>
        <th>{{.WP.TrackLength}}m time</th>
        <th>Speed</th>
        <th>Pace</th>
      </tr>
    </thead>
    <tbody>
      {{range .VMAs}}
      <tr>
        <td><b>{{.VMA}}</b></td>
        <td>{{.TotalTime}}</td>
        <td>{{.TimeTrack}}</td>
        <td>{{.Speed}}</td>
        <td>{{.Pace}}</td>
      </tr>
      {{end}}
    </tbody>
    </table>
  </div>
  <div>
    <p class="text-right text-warning" >Repos: {{.WP.Repos}} </p>
  </div>
